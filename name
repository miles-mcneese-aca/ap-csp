#include <stdio.h>
#include <string.h>  // Include this for strcmp

int main() {
    char name[] = "Miles";
    char entry[20];

    scanf("%s", entry);

    // Use strcmp to compare strings
    if (strcmp(entry, name) == 0) {
        printf("Welcome %s\n", name);
    } else {
        printf("I don't know you\n");
    }

    return 0;
}  
